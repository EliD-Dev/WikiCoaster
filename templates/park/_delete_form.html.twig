<form method="post" action="{{ path('app_park_delete', {'id': park.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer ce park ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ park.id) }}">
    <button class="btn btn-danger mb-3">Delete</button>
</form>
